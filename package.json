{
  "name": "stremio-desktop",
  "version": "0.1.0",
  "description": "Electron wrapper for stremio-web.",
  "author": "tymmesyde",
  "main": "src/main",
  "private": true,
  "scripts": {
    "start": "cross-env PORT=8181 run-p vue:serve electron:serve",
    "build": "run-s vue:build electron:build package",
    "vue:serve": "cross-env-shell vue-cli-service serve --port $PORT",
    "vue:build": "vue-cli-service build",
    "electron:serve": "cross-env-shell wait-on http-get://localhost:$PORT && cross-env-shell electron . --port $PORT",
    "electron:build": "webpack",
    "package": "node scripts/package"
  },
  "dependencies": {
    "@ionic/core": "^5.5.4",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "change-case": "^4.1.2",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "electron": "^11.2.3",
    "electron-serve": "^1.1.0",
    "electron-window-state": "^5.0.3",
    "express": "^4.17.1",
    "ionicons": "^5.4.0",
    "minimist": "^1.2.5",
    "pump": "^3.0.0",
    "range-parser": "^1.2.1",
    "torrent-stream": "^1.2.1",
    "unzipper": "^0.10.11",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "create-file-webpack": "^1.0.2",
    "cross-env": "^7.0.3",
    "electron-packager": "^15.2.0",
    "electron-reloader": "^1.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "wait-on": "^5.2.1",
    "webpack": "^4.45.0",
    "webpack-cli": "^4.5.0"
  }
}
